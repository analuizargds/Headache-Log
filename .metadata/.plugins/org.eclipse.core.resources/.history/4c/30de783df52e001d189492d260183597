package model;

public class Person {
	private String name;
	private String sex;
	
	public Person(String name, String sex) {
		this.setName(name);
		this.setSex(sex);
	}
	
	public Person() {
		
	}

	public String getSex() {
		return sex;
	}

	public void setSex(String sex) {
		this.sex = sex;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
}
