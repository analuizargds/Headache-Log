package model;

import java.util.Date;

public class Medicine {
	private String name;
    private double dosage;
    private String unit ;
    private Date date;
    
    public Medicine(String name, double dosage, String unit){//, String unit, String route)
        this.name = name;
        this.dosage = dosage;
        this.unit = unit;
        this.date = date;
    }

    public Medicine() {
    }

    public Medicine(String paracetamol, String string, String comprimido) {
 
    }
    
    public String getName(){
        return name;
    }
    
    public void setName(String name){
        this.name = name;
    }
    public Double getDosage(){
        return dosage;
    }
    
    public void setDosage(double dosage){
        this.dosage = dosage;
    }
    public String getUnit(){
        return unit;
    }
    
    public void setUnit(String unit){
        this.unit = unit;
    }
    
    public Date getDate() {
    	return date;
    }
    
    public void setDate(Date date) {
    	this.date = date;
    }

}
