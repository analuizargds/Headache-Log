package test;

import static org.junit.jupiter.api.Assertions.*;

import org.junit.jupiter.api.Test;

import controler.ControlerMedicine;
import controler.ControlerPain;

class ControlerMedicineTest {

	@Test
	void testCommonArea() {
		ControlerMedicine cm = new ControlerMedicine();
		String resultTest = "Ibuprofeno";
		String result = cm.commonMedicine();
		assertTrue(resultTest.equals(result));
	}

}
